{
  "welcomeFile": "/webapp/index.html",
  "routes": [
	{
		"path": "/contentstack_proxy",
		"target": {
			"type": "destination",
			"name": "contentstack_proxy",
			"entryPath": "/"
		},
		"description": "Built Proxy Java application"
	},
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    }
  ],
  "sendWelcomeFileRedirect": true,
  "cacheControl": [
    {
      "directive": "public",
      "maxAge": 0
    }
  ]
}